{
  "name": "express-typescript-starter",
  "description": "Template to start developing a REST API with Node.js (Express), TypeScript, ESLint, Prettier, PostgreSQL, Objection, etc.",
  "version": "0.0.0",
  "homepage": "https://github.com/mfaizshahid/express-typescript-starter",
  "license": "MIT",
  "main": "./dist/app.js",
  "author": {
    "name": "Muhammad Faiz Shahid",
    "url": "https://twitter.com/mfaizshahid",
    "email": "mfaizshahid.me@gmail.com"
  },
  "scripts": {
    "dev": "nodemon",
    "start": "npm run build && node  ./dist/src/app.js",
    "build": "npm run build:compile && npm run build:assets",
    "build:compile": "rimraf dist && tsc -p tsconfig.build.json && tsc-alias -p tsconfig.build.json --silent",
    "build:assets": "copyfiles --error -f package*.json dist/",
    "clean": "rimraf dist",
    "check:format": "prettier --check .",
    "check:lint": "eslint . --color",
    "fix:format": "prettier --check --write --ignore-unknown .",
    "fix:lint": "npm run check:lint -- --fix"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/mfaizshahid/express-typescript-starter"
  },
  "engines": {
    "node": ">=20.9.0",
    "npm": ">=6.7.0"
  },
  "dependencies": {
    "bcrypt": "^5.1.1",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "http-status": "^1.7.3",
    "joi": "^17.11.0",
    "jsonwebtoken": "^9.0.2",
    "knex": "^3.1.0",
    "moment": "^2.29.4",
    "morgan": "^1.10.0",
    "objection": "^3.1.3",
    "pg": "^8.11.3",
    "winston": "^3.11.0",
    "winston-daily-rotate-file": "^4.7.1"
  },
  "devDependencies": {
    "@types/bcrypt": "^5.0.2",
    "@types/cors": "^2.8.17",
    "@types/eslint": "^8.56.0",
    "@types/express": "^4.17.21",
    "@types/jsonwebtoken": "^9.0.5",
    "@types/morgan": "^1.9.9",
    "@types/node": "^20.10.4",
    "@types/pg": "^8.10.9",
    "@typescript-eslint/eslint-plugin": "^6.16.0",
    "@typescript-eslint/parser": "^6.16.0",
    "copyfiles": "^2.4.1",
    "eslint": "^8.56.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-airbnb-typescript": "^17.1.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-n": "^16.4.0",
    "eslint-plugin-prettier": "^5.1.2",
    "eslint-plugin-promise": "^6.1.1",
    "eslint-plugin-simple-import-sort": "^10.0.0",
    "eslint-plugin-unused-imports": "^3.0.0",
    "husky": "^8.0.3",
    "lint-staged": "^15.2.0",
    "nodemon": "^3.0.2",
    "prettier": "^3.1.1",
    "ts-node": "^10.9.2",
    "tsc-alias": "^1.8.8",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.3.3"
  }
}
